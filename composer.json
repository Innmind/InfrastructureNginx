{
    "name": "innmind/infrastructure-nginx",
    "type": "library",
    "description": "Tool to install nginx",
    "keywords": ["nginx"],
    "homepage": "http://github.com/Innmind/InfrastructureNginx",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/InfrastructureNginx/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/cli": "~2.0",
        "innmind/operating-system": "~2.0",
        "innmind/installation-monitor": "~3.0",
        "innmind/silent-cartographer": "~2.0",
        "innmind/genome": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Infrastructure\\Nginx\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Infrastructure\\Nginx\\": "tests/",
            "Fixture\\Innmind\\Infrastructure\\Nginx\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "giorgiosironi/eris": "^0.10.0",
        "vimeo/psalm": "^3.10"
    },
    "bin": ["infrastructure-nginx"],
    "provide": {
        "innmind/genome": "3.0"
    }
}
